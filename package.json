{
  "name": "generator-nifty-az",
  "version": "0.1.3",
  "description": "A collection of useful Azure apps generators",
  "homepage": "https://github.com/dbroeglin/generator-nifty-az",
  "author": {
    "name": "Dominique Broeglin",
    "email": "dominique.broeglin@gmail.com",
    "url": "https://broeglin.fr"
  },
  "files": [
    "generators"
  ],
  "type": "module",
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "scaffolding",
    "app",
    "Azure",
    "demo",
    "sample",
    "inception",
    "boilerplate",
    "front-end",
    "back-end"
  ],
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^8.2.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.12",
    "tmp": "^0.2.3",
    "yeoman-generator": "^7.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "dbroeglin/generator-nifty-az",
  "license": "MIT"
}
